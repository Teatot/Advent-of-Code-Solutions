def FileRead (name):
    with open(name, 'r') as data:
        l = ""
        for line in data.readlines():
            l += str(line)
    return l.split("\n")
    
    
def Solution_4 (list):
    hor, dep, aim = 0, 0, 0
    for seq in list:
        seq = seq.split()
        if seq[0] == 'forward':
            hor += int(seq[1])
            dep += (int(seq[1]) * aim)
        elif seq[0] == 'up':
            aim -= int(seq[1])
        elif seq[0] == "down":
            aim += int(seq[1])
    return hor * dep
