def FileRead (name):
    with open(name, 'r') as data:
        s = []
        for num in data.readlines():
            if num != '\n':
                s.append(int(num))
    return s
    
def Solution_Two (lst):
    incr = 0
    a, b = 0, 3
    while b < len(lst):
        sum_a = sum([lst[i] for i in range(a, b)])
        sum_b = sum([lst[i] for i in range(a + 1, b + 1)])
        if sum_b - sum_a > 0:
            incr += 1
        a += 1
        b += 1
    return incr
