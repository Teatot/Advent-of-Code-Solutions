def FileRead (name):
    with open(name, 'r') as data:
        l = ""
        for line in data.readlines():
            l += str(line)
    return l.split("\n")
    
def Solution_3  (list):
    hor, dep = 0, 0
    for seq in list:
        seq = seq.split()
        if seq[0] == 'forward':
            hor += int(seq[1])
        elif seq[0] == 'up':
            dep -= int(seq[1])
        elif seq[0] == "down":
            dep += int(seq[1])
    return hor * dep
